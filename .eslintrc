{
  "env": {
    "browser": true,
    "node": true,
    "es2017": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    //"ecmaVersion": 2017,
    "sourceType": "module"
  },
  "globals": {
    "jQuery": "readonly",
    "$": "readonly"
  },
  "rules": {
    // https://eslint.org/docs/rules/
    // excentesで全体的に設定したものを細かく設定
    "semi": ["error", "never"]
  }
}
